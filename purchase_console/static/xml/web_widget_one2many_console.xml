<templates>
    <t t-name="ListView.row.one2many_columns">
        <t t-set="ele" t-value="widget.elements"/>
        <t t-foreach="widget.elements.length" t-as="index">
           <ul class="list-group">
              <li class="list-group-item">
                  <t t-esc="ele[index].partner_id[1]"></t>
              </li>
              <li class="list-group-item">
                  <t t-esc="Math.round(ele[index].price_bid, -2)"></t>
              </li>
              <li class="list-group-item">
                  <t t-esc="Math.round(ele[index].last_price, -2)"></t>
              </li>
              <li class="list-group-item">
                  <t t-esc="Math.round(ele[index].price_unit, -2)"></t>
              </li>
           </ul>
        </t>
    </t>
    <t role="group" aria-label="..." t-name="ListView.row.one2many_columns_place">
        <div  t-att-class="widget.name + '_' + record.id.value">
        </div>
    </t>
</templates>